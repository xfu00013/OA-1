<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_workflow" name="com.oa.model.Workflow">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <property name="name" access="field" unique="true"/>
    <property name="processDefinition" length="99999999" access="field" type="binary"/>
    <property name="processImage" length="99999999" access="field" type="binary"/>
  </class>
</hibernate-mapping>
